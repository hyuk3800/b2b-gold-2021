mixin table(product)
            tbody 
                tr 
                    td(rowspan="2")
                        input(name="clickThis" class="clickThis" type="checkbox",  value=`${product._id}`)
                    td(rowspan="2")
                        div(class="model")
                            img(src="/" + product.fileUrl, alt="catalogeImg")
                        div(class="mdName")
                            span=product.title
                    td(rowspan="2")
                        span 함량
                    td
                        span #{product.goldWeight}
                    td
                        span 개당알수(메인)
                    td
                        span #{product.basicWage}
                    td
                        span #{product.stoneWage1}
                    td(rowspan="2")
                        span 수량
                    td(rowspan="2")
                        span #{product.basicWage + product.stoneWage1 + product.additionWage + product.stoneWage2}
                    td(rowspan="2")
                        span 금값
                    td(rowspan="2")
                        span 소계
                tr 
                    td #{product.stoneWeight}
                    td 게당알수 (서브)
                    td #{product.additionWage}
                    td #{product.stoneWage2}